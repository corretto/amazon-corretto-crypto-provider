BasedOnStyle:  WebKit
---
Language:        Cpp
AlignConsecutiveMacros: true
AlignOperands:   Align
AlignTrailingComments: true
AllowAllParametersOfDeclarationOnNextLine: true
BinPackParameters: false
ColumnLimit:     120
IncludeCategories:
  - Regex:           '^<openssl/'
    Priority:        2
    SortPriority:    0
  - Regex:           '^"(llvm|llvm-c|clang|clang-c)/'
    Priority:        3
    SortPriority:    0
  - Regex:           '^(<|"(gtest|gmock|isl|json)/)'
    Priority:        4
    SortPriority:    0
  - Regex:           '.*'
    Priority:        1
    SortPriority:    0
MaxEmptyLinesToKeep: 3
WhitespaceSensitiveMacros:
  - STRINGIZE
  - PP_STRINGIZE
  - BOOST_PP_STRINGIZE
  - TO_STRING
  - TO_STRING_0
  - CONCAT2
  - CONCAT2_INTERNAL
...

